<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../../../../../msbuild/ice.common.props" />
  <PropertyGroup>
    <AssemblyName>user</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <OutputPath>.</OutputPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <SliceCompile>
      <IncludeDirectories>../../..</IncludeDirectories>
    </SliceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <PackageReference Include="zeroc.icebuilder.msbuild" Version="5.0.4" />
  </ItemGroup>
  <ItemGroup>
    <SliceCompile Include="../../../User.ice" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../core/netstandard2.0/core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="generated\User.cs">
      <SliceCompileSource>../../../User.ice</SliceCompileSource>
    </Compile>
  </ItemGroup>
  <Choose>
    <When Condition="'$(ICE_BIN_DIST)' == 'all'">
      <ItemGroup>
        <PackageReference Include="zeroc.ice.net" Version="3.7.2" />
      </ItemGroup>
    </When>
    <Otherwise>
      <ItemGroup>
        <Reference Include="../../../../../../lib/netstandard2.0/Ice.dll" />
      </ItemGroup>
    </Otherwise>
  </Choose>
</Project>
